<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
 "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="SMP000">

	<!-- データクリア -->
	<delete id="deleteNyusyukkin">
		DELETE FROM NYUSYUKKINTBL
	</delete>

	<!-- データ挿入 -->
	<!-- 入出金テーブル -->
	<insert id="insertNyusyukkin"
		parameterClass="jp.terasoluna.batch.tutorial.common.NyusyukkinData">
		INSERT
		INTO
		NYUSYUKKINTBL (
		SHITENNAME
		,KOKYAKUID
		,NYUSYUKKINKUBUN
		,KINGAKU
		,TORIHIKIBI
		)
		VALUES (
		#shitenName#
		,#kokyakuId#
		,#nyusyukkinKubun#
		,#kingaku#
		,#torihikibi#
		)
	</insert>
</sqlMap>