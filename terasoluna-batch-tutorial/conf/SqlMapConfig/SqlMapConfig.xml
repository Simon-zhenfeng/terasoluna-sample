	<sqlMap resource="SqlMapConfig/SMP001_sqlMap.xml" />
	<sqlMap resource="SqlMapConfig/SMP002_sqlMap.xml" />
	<sqlMap resource="SqlMapConfig/SMP003_sqlMap.xml" />
	<sqlMap resource="SqlMapConfig/SMP005_sqlMap.xml" />
</sqlMapConfig>
