<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="jp.terasoluna.batch.functionsample.b007.b007002.B007002Dao">

    <select id="collectEmployee"
        resultType="jp.terasoluna.batch.functionsample.b007.CsvRecord">
        SELECT
        EMPLOYEE_ID AS "id"
        ,EMPLOYEE_FAMILY_NAME AS "familyName"
        ,EMPLOYEE_FIRST_NAME AS "firstName"
        ,EMPLOYEE_AGE AS "age"
        FROM
        EMPLOYEE
    </select>

</mapper>