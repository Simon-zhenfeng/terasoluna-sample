<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="B009002">

	<select id="selectEmployees" resultClass="jp.terasoluna.batch.functionsample.b009.CsvRecord">
		SELECT
		employee_id as id
		,employee_family_name as familyName
		,employee_first_name as firstName
		,employee_age as age
		FROM
		employee3
	</select>

	<insert id="insertEmployee2" parameterClass="jp.terasoluna.batch.functionsample.b009.CsvRecord">
		INSERT
		INTO
		employee2
		(
		employee_id
		,employee_family_name
		,employee_first_name
		,employee_age
		)
		VALUES (
		#id#
		,#familyName#
		,#firstName#
		,#age#
		)
		</insert>

</sqlMap>